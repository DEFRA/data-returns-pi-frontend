{
  "name": "data-returns-pi-frontend",
  "version": "1.0.0",
  "description": "Data Returns: Pollution Inventory Service",
  "main": "server.js",
  "engines": {
    "node": "8.6.0"
  },
  "dependencies": {
    "dotenv": "4.0.0",
    "js-yaml": "3.10.0",
    "winston": "2.4.0",
    "hapi": "16.6.2",
    "inert": "4.2.1",
    "good": "7.3.0",
    "crumb": "6.0.3",
    "good-winston": "4.0.0",
    "vision": "4.1.1",
    "klaw": "2.1.0",
    "gaze": "1.1.2",
    "minimatch": "3.0.4",
    "node-sass": "4.5.3",
    "node-minify": "2.3.1",
    "catbox-redis": "3.1.1",
    "hapi-auth-cookie": "7.0.0",
    "uuid": "3.1.0",
    "time-stamp": "2.0.0",
    "fs-extra": "4.0.2",
    "nunjucks": "3.0.1",
    "lodash": "4.17.4",
    "joi": "13.0.1",
    "govuk-elements-sass": "3.1.2",
    "govuk_frontend_toolkit": "7.1.0",
    "govuk_template_jinja": "0.23.0"
  },
  "devDependencies": {
    "eslint": "4.9.0",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-node": "5.2.1",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-standard": "3.0.1",
    "lab": "15.1.0",
    "code": "^5.1.0",
    "nyc": "11.2.1"
  },
  "nyc": {
    "include": [
      "app/**"
    ],
    "reporter": [
      "text",
      "text-summary"
    ]
  },
  "scripts": {
    "start": "npm test && node main.js",
    "proto": "node proto.js",
    "lint": "./node_modules/eslint/bin/eslint.js .",
    "test": "./node_modules/lab/bin/lab --timeout 10000 -e local --assert code",
    "coverage": "nyc --all npm start",
    "coverage-unittest": "nyc --all npm test"
  },
  "keywords": [
    "Data Returns",
    "Pollution Inventory",
    "GDS"
  ],
  "author": "Druid Wood Limited / Sam Gardner-Dell",
  "license": "ISC",
  "repository": "https://github.com/DEFRA/data-returns-pi-frontend.git"
}
