{
  "name": "data-returns-pi-frontend",
  "version": "1.0.0",
  "description": "Data Returns: Pollution Inventory Service",
  "main": "server.js",
  "engines": {
    "node": "8.9.1"
  },
  "dependencies": {
    "boom": "7.1.1",
    "catbox": "10.0.2",
    "catbox-redis": "4.0.0",
    "crumb": "7.0.0",
    "dotenv": "5.0.0",
    "fs-extra": "5.0.0",
    "gaze": "1.1.2",
    "good": "8.0.1",
    "govuk-elements-sass": "3.1.2",
    "govuk_frontend_toolkit": "7.2.0",
    "govuk_template_jinja": "0.23.0",
    "hapi": "17.2.0",
    "hapi-auth-cookie": "8.0.0",
    "hapi-good-winston": "2.0.0-rc1",
    "inert": "5.1.0",
    "joi": "13.1.2",
    "js-yaml": "3.10.0",
    "klaw": "2.1.1",
    "lodash": "4.17.5",
    "minimatch": "3.0.4",
    "node-minify": "2.4.1",
    "node-sass": "4.7.2",
    "nunjucks": "3.0.1",
    "request": "2.83.0",
    "request-promise": "4.2.2",
    "time-stamp": "2.0.0",
    "uri-js": "^3.0.2",
    "uuid": "^3.2.1",
    "vision": "5.3.1",
    "winston": "2.4.0"
  },
  "devDependencies": {
    "code": "5.1.2",
    "eslint": "4.17.0",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-node": "6.0.0",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-standard": "3.0.1",
    "lab": "15.2.1",
    "nyc": "11.4.1"
  },
  "nyc": {
    "include": [
      "app/**"
    ],
    "reporter": [
      "text",
      "text-summary"
    ]
  },
  "scripts": {
    "start": "node main.js",
    "proto": "node proto.js",
    "lint": "./node_modules/eslint/bin/eslint.js .",
    "test": "redis-cli flushdb && ./node_modules/lab/bin/lab -e localtest --assert code",
    "test-md-api": "./node_modules/lab/bin/lab -e local --assert code --coverage-path './src/model' --coverage-exclude ./test/model/master-data-api.js",
    "clear-cache": "redis-cli flushdb",
    "coverage": "nyc --all npm start",
    "coverage-unittest": "nyc --all npm test",
    "docker-build": "docker build -t defra/data-returns-pi-frontend . && docker images"
  },
  "keywords": [
    "Data Returns",
    "Pollution Inventory",
    "GDS"
  ],
  "author": "Druid Wood Limited",
  "license": "ISC",
  "repository": "https://github.com/DEFRA/data-returns-pi-frontend.git"
}
